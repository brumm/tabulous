{
  "name": "tabulous",
  "version": "0.0.5",
  "description": "a chrome extension thingie to wrangle your tabs",
  "scripts": {
    "release": "npm run bump && npm run build && npm run zip",
    "build": "NODE_ENV='production' node utils/build.js",
    "start": "node utils/webserver.js",
    "zip": "zip -r -j build ./build",
    "bump": "npm version patch -m '%s'"
